<footer id="footer">
    <div class="inner">
        <section>
            <h2>Get in touch</h2>
            {% if page.header.footer.form_enabled  %}
                {% include "forms/form.html.twig" %}
            {% endif %}
        </section>
        <section>
            <h2>Follow</h2>
            <ul class="icons">
                {% set links = page.header.footer.links %}
                {% for fa,link in links %}
                    <li><a href="{{ link }}" class="icon style2 {{ fa }}"><span class="label">Twitter</span></a></li>
                {% endfor %}
            </ul>
        </section>
        <ul class="copyright">
            <li>&copy; Untitled. All rights reserved</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
        </ul>
    </div>
</footer>